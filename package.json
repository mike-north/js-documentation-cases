{
  "name": "js-documentation-cases",
  "version": "0.0.0-development",
  "description": "Test cases for JavaScript & TypeScript library documentation solutions",
  "main": "index.js",
  "repository": "https://github.com/mike-north/js-documentation-cases",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.works)",
  "license": "BSD-2-Clause",
  "private": true,
  "scripts": {
    "test": "yarn build",
    "run_for_each": "scripty",
    "build:typedoc": "scripty",
    "build:esdoc": "scripty",
    "build:jsdoc": "scripty",
    "build:ts-docs-gen": "scripty",
    "build:docco": "scripty",
    "build:yuidoc": "scripty",
    "build:api-extractor": "scripty",
    "build:code-to-json": "scripty",
    "build:api-extractor:create-config": "scripty",
    "build:yuidoc:create-config": "SCRIPTY_SILENT=true scripty",
    "build:index": "scripty",
    "build:ts": "scripty",
    "build:docs": "scripty",
    "build": "scripty",
    "postinstall": "scripty",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@code-to-json/cli": "1.0.0-rc.42",
    "@microsoft/api-documenter": "7.4.0",
    "@microsoft/api-extractor": "7.3.11",
    "@microsoft/tsdoc": "0.12.14",
    "@mike-north/js-lib-renovate-config": "1.2.0",
    "@semantic-release/git": "7.0.16",
    "@types/commander": "2.12.2",
    "@types/fs-extra": "7.0.0",
    "@types/handlebars": "4.0.40",
    "@types/lodash": "4.14.141",
    "@types/node": "10.14.20",
    "commander": "2.20.1",
    "docco": "0.8.0",
    "esdoc": "1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "esdoc-typescript-plugin": "1.0.1",
    "fs-extra": "8.0.1",
    "handlebars": "4.1.2",
    "jsdoc": "3.6.1",
    "scripty": "1.8.0",
    "semantic-release": "15.13.18",
    "ts-docs-gen": "0.2.0",
    "ts-jsdoc": "3.1.1",
    "ts-node": "8.0.3",
    "tslint": "5.17.0",
    "typedoc": "0.14.2",
    "typescript": "3.5.1",
    "yuidocjs": "0.10.2"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/github"
    ],
    "analyzeCommits": [
      "@semantic-release/commit-analyzer"
    ],
    "prepare": [
      {
        "path": "@semantic-release/git",
        "assets": [
          "docs"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/github"
    ],
    "success": [
      "@semantic-release/github"
    ],
    "fail": [
      "@semantic-release/github"
    ]
  }
}
