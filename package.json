{
  "name": "js-documentation-cases",
  "version": "0.0.0-development",
  "description": "Test cases for JavaScript & TypeScript library documentation solutions",
  "main": "index.js",
  "repository": "https://github.com/mike-north/js-documentation-cases",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.works)",
  "license": "BSD-2-Clause",
  "private": false,
  "scripts": {
    "test": "yarn build",
    "run_for_each": "scripty",
    "build:typedoc": "scripty",
    "build:esdoc": "scripty",
    "build:jsdoc": "scripty",
    "build:ts-docs-gen": "scripty",
    "build:docco": "scripty",
    "build:yuidoc": "scripty",
    "build:yuidoc:create-config": "SCRIPTY_SILENT=true scripty",
    "build:index": "scripty",
    "build:ts": "scripty",
    "build:docs": "scripty",
    "build": "scripty",
    "postinstall": "scripty",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/git": "^7.0.4",
    "@types/handlebars": "^4.0.39",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.11.7",
    "docco": "^0.8.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "handlebars": "^4.0.12",
    "jsdoc": "^3.5.5",
    "scripty": "^1.7.2",
    "semantic-release": "^15.9.17",
    "travis-deploy-once": "^5.0.9",
    "ts-docs-gen": "^0.2.0",
    "ts-jsdoc": "^3.0.1",
    "typedoc": "^0.12.0",
    "typescript": "^3.1.2",
    "yuidocjs": "^0.10.2"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/github"
    ],
    "analyzeCommits": [
      "@semantic-release/commit-analyzer"
    ],
    "prepare": [
      {
        "path": "@semantic-release/git",
        "assets": [
          "docs"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/github"
    ],
    "success": [
      "@semantic-release/github"
    ],
    "fail": [
      "@semantic-release/github"
    ]
  }
}
