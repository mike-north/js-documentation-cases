{
  "name": "js-documentation-cases",
  "version": "0.0.0-development",
  "description": "Test cases for JavaScript & TypeScript library documentation solutions",
  "main": "index.js",
  "repository": "https://github.com/mike-north/js-documentation-cases",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.works)",
  "license": "BSD-2-Clause",
  "private": true,
  "scripts": {
    "test": "yarn build",
    "run_for_each": "scripty",
    "build:typedoc": "scripty",
    "build:esdoc": "scripty",
    "build:jsdoc": "scripty",
    "build:ts-docs-gen": "scripty",
    "build:docco": "scripty",
    "build:yuidoc": "scripty",
    "build:api-extractor": "scripty",
    "build:code-to-json": "scripty",
    "build:api-extractor:create-config": "scripty",
    "build:yuidoc:create-config": "SCRIPTY_SILENT=true scripty",
    "build:index": "scripty",
    "build:ts": "scripty",
    "build:docs": "scripty",
    "build": "scripty",
    "postinstall": "scripty",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@code-to-json/cli": "0.4.28",
    "@microsoft/api-documenter": "1.5.59",
    "@microsoft/api-extractor": "6.3.0",
    "@microsoft/tsdoc": "0.12.4",
    "@mike-north/js-lib-renovate-config": "1.2.0",
    "@semantic-release/git": "7.0.4",
    "@types/commander": "2.12.2",
    "@types/fs-extra": "5.0.4",
    "@types/handlebars": "4.0.39",
    "@types/lodash": "4.14.119",
    "@types/node": "10.12.12",
    "commander": "2.19.0",
    "docco": "0.8.0",
    "esdoc": "1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "esdoc-typescript-plugin": "1.0.1",
    "fs-extra": "7.0.1",
    "handlebars": "4.0.12",
    "jsdoc": "3.5.5",
    "scripty": "1.7.2",
    "semantic-release": "15.9.17",
    "ts-docs-gen": "0.2.0",
    "ts-jsdoc": "3.0.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typedoc": "0.13.0",
    "typescript": "3.2.1",
    "yuidocjs": "0.10.2"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/github"
    ],
    "analyzeCommits": [
      "@semantic-release/commit-analyzer"
    ],
    "prepare": [
      {
        "path": "@semantic-release/git",
        "assets": [
          "docs"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/github"
    ],
    "success": [
      "@semantic-release/github"
    ],
    "fail": [
      "@semantic-release/github"
    ]
  }
}
